{
	"name" : "Emmet Re:View",
	"version" : "1.0.0",
	"description" : "Shows views with all media-query breakpoints for current page",
	"manifest_version": 2,
	"permissions" : [
		"activeTab",
		"storage",
		"http://*/*",
		"https://*/*"
	],
	"browser_action": {
		"default_title": "Show breakpoint views"
	},
	"background": {
		"scripts": ["background.js"]
	}
}