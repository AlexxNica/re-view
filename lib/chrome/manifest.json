{
	"name" : "Emmet Re:View",
	"version" : "1.0.0",
	"description" : "Shows views with all responsive media-query breakpoints for current page",
	"manifest_version": 2,
	"permissions" : [
		"activeTab",
		"storage",
		"clipboardWrite",
		"http://*/*",
		"https://*/*"
	],
	"icons": {
		"16": "16x16.png",
		"48": "48x48.png",
		"128": "app-icon.png"
	},
	"browser_action": {
		"default_title": "Emmet Re:View",
		"default_icon": {
			"19": "19x19.png",
			"38": "38x38.png"
		}
	},
	"background": {
		"scripts": ["background.js"]
	}
}